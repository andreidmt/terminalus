{
  "name": "terminalus",
  "version": "0.0.1",
  "description": "Terminal dashboard for npm",
  "main": "dist/index.js",
  "scripts": {
    "update": "npm-check -u",
    "setup": "rm -f package-lock.json && rm -rf ./node_modules && npm i",
    "start": "DEBUG=Terminalus* babel-node source/index.js",
    "lint": "eslint source/",
    "lint:fix": "eslint_d --color --fix source/",
    "test": "tape -r babel-register source/**/*.test.js",
    "build": "rm -rf ./dist && babel source --out-dir dist"
  },
  "author": "Andrei Dumitrescu <andrei@codemachiner.com> (https://twitter.com/andreidcm)",
  "license": "ISC",
  "devDependencies": {
    "@codemachiner/eslint-config": "2.7.2",
    "babel-cli": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-preset-env": "1.6.0",
    "debug": "3.0.0",
    "eslint": "4.5.0",
    "eslint-plugin-compat": "1.0.4",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-unicorn": "2.1.2",
    "eslint-watch": "3.1.2",
    "markdownlint-cli": "0.3.1",
    "npm-check": "5.4.5",
    "tap-diff": "0.1.1",
    "tape": "4.8.0"
  },
  "dependencies": {
    "ajv": "5.2.2",
    "blessed": "0.1.81",
    "chalk": "2.1.0",
    "figures": "2.0.0",
    "immutable": "3.8.1",
    "node-notifier": "5.1.2",
    "ramda": "0.24.1",
    "rc": "1.2.1"
  }
}
